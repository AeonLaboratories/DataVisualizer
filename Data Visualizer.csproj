<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	<AssemblyVersion>1.5.0.0</AssemblyVersion>
	<PackageVersion>$(AssemblyVersion)</PackageVersion>
	<Title>Data Visualizer</Title>
	<Description>View data generated from an AeonHacs application.</Description>
	<Company>Aeon Laboratories</Company>
	<Authors>Jim Wilson, Tyler Wilson</Authors>
	<Copyright>$(Company), 2025</Copyright>
	<AssemblyName>Data Visualizer</AssemblyName>
	<RepositoryUrl>https://github.com/AeonLaboratories/DataVisualizer</RepositoryUrl>
	<RootNamespace>Data_Visualizer</RootNamespace>

	<UseWindowsForms>true</UseWindowsForms>
	<OutputType>WinExe</OutputType>
	<TargetFramework>net9.0-windows</TargetFramework>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageLicenseFile>LICENSE</PackageLicenseFile>
    <ApplicationIcon>DataVisualizer.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="DataVisualizer.ico" />
  </ItemGroup>
</Project>